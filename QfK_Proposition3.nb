Cell[BoxData[{
 RowBox[{
  RowBox[{
   SubscriptBox["V", 
    RowBox[{"deepen", ",", " ", 
     RowBox[{"X", ">", 
      RowBox[{"8", "q"}]}], ",", " ", 
     RowBox[{"d", "<", 
      RowBox[{"4", "q"}]}]}]], "[", 
   RowBox[{"d_", ",", "X_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"1", "/", 
    RowBox[{"(", 
     RowBox[{"6", "q"}], ")"}]}], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"2", " ", "X", " ", 
      RowBox[{
       SubscriptBox[
        RowBox[{"(", 
