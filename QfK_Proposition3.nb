         RowBox[{"4", "q"}]}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"3", "/", "2"}], ")"}]}]}], "+", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"X", "-", "d"}], "]"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"X", "-", "d", "-", 
         RowBox[{"4", "q"}]}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"3", "/", "2"}], ")"}]}]}]}], ")"}]}]}], "\n", 
 RowBox[{
  RowBox[{
   SubscriptBox["Ratio", 
    RowBox[{"deepen", ",", " ", 
     RowBox[{"X", ">", 
      RowBox[{"8", "q"}]}], ",", " ", 
     RowBox[{"d", "<", 
      RowBox[{"4", "q"}]}]}]], "[", 
   RowBox[{"d_", ",", "X_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    SubscriptBox["V", 
     RowBox[{"deepen", ",", " ", 
      RowBox[{"X", ">", 
       RowBox[{"8", "q"}]}], ",", " ", 
      RowBox[{"d", "<", 
       RowBox[{"4", "q"}]}]}]], "[", 
    RowBox[{"d", ",", "X"}], "]"}], "/", 
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      SuperscriptBox["\[Sigma]", "2"], ")"}], "deepen"], "[", 
    RowBox[{"d", ",", "X"}], "]"}]}]}]}], "Input",
